    "commands": ["workbench.view.explorer"],
  
  },
  {
        "before": [";"],
    "after": ["$"]

  }  ,
  {
    "before": ["j"],
    "after": ["k"]
  },
{

      "before": ["k"],
    "after": ["j"]
},
  {
